<?xml version="1.0" encoding="utf-8"?>
<data-extraction-rules>
    <cloud-backup>
        <!-- Include preferences and user scripts -->
        <include domain="sharedpref" path="."/>
        <include domain="file" path="scripts/"/>
    </cloud-backup>

    <device-transfer>
        <!-- Allow transferring user data to new device -->
        <include domain="sharedpref" path="."/>
        <include domain="file" path="scripts/"/>
    </device-transfer>
</data-extraction-rules>